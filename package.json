  "keywords": [
    "ebms",
    "biological",
    "recording",
    "mobile",
    "app",
    "offline",
    "ceh",
    "ukbms"
  ],
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "android": "5.1",
            "ios": "10.2"
          },
          "modules": "commonjs",
          "corejs": "3.0.0",
          "useBuiltIns": "usage"
        }
      ],
      "@babel/react",
      "@babel/typescript"
    ],
    "plugins": [
      [
        "@babel/transform-runtime",
        {
          "regenerator": true
        }
      ],
      [
        "module-resolver",
        {
          "root": [
            "./src"
          ],
          "alias": {
            "@apps": "./src/common/appsBitCollection",
            "common": "./src/common",
            "helpers": "./src/common/helpers",
            "models": "./src/common/models",
            "appModel": "./src/common/models/appModel",
            "userModel": "./src/common/models/userModel",
            "savedSamples": "./src/common/models/savedSamples",
            "sample": "./src/common/models/sample",
            "occurrence": "./src/common/models/occurrence",
            "config": "./src/common/config/config",
            "Components": "./src/common/Components"
          }
        }
      ],
      [
        "@babel/proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/transform-exponentiation-operator",
      [
        "@babel/proposal-private-methods",
        {
          "loose": true
        }
      ],
      [
        "@babel/proposal-class-properties",
        {
          "loose": true
        }
      ],
      "@babel/syntax-dynamic-import"
    ]
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "parser": "@typescript-eslint/parser",
    "extends": [
      "@flumens",
      "plugin:@typescript-eslint/recommended"
    ],
    "settings": {
      "import/resolver": {
        "babel-module": {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      }
    },
    "parserOptions": {
      "project": [
        "./tsconfig.json"
      ]
    },
    "plugins": [
      "@typescript-eslint"
    ],
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "jsx-a11y/accessible-emoji": 0,
      "sort-class-members/sort-class-members": 0,
      "@getify/proper-arrows/name": 0,
      "@typescript-eslint/no-unused-vars": "error",
      "@typescript-eslint/no-var-requires": 0,
      "import/extensions": [
        "error",
        "ignorePackages",
        {
          "js": "never",
          "jsx": "never",
          "ts": "never",
          "tsx": "never"
        }
      ],
      "@typescript-eslint/no-empty-interface": "off",
      "no-use-before-define": "off",
      "@typescript-eslint/no-use-before-define": [
        "error"
      ],
      "react/prop-types": "off",
      "react/jsx-filename-extension": [
        2,
        {
          "extensions": [
            ".js",
            ".jsx",
            ".ts",
            ".tsx"
          ]
        }
      ]
    },
    "globals": {
      "t": true
    }
  },
  "prettier": {
    "printWidth": 80,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "trailingComma": "es5",
    "arrowParens": "avoid"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/compilers/react@2.0.0"
    },
    "componentsDefaultDirectory": "components/{name}",
    "packageManager": "npm"
  }
}
